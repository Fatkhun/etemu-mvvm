    <style name="Base.Theme.Etemu" parent="Theme.MaterialComponents.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/p_50</item>
        <item name="colorPrimaryVariant">@color/p_50</item>
        <item name="colorOnPrimary">@color/b_0</item>
        <item name="colorPrimaryDark">@color/p_50</item>

